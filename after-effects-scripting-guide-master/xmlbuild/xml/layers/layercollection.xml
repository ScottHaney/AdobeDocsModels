<inline classes="keyword declaration">var</inline> <inline classes="name other">layerCollection</inline> <inline classes="operator">=</inline> <inline classes="name other">firstComp</inline><inline classes="punctuation">.</inline><inline classes="name other">layers</inline><inline classes="punctuation">;</inline>
<inline classes="name other">alert</inline><inline classes="punctuation">(</inline><inline classes="literal string double">"number of layers before is "</inline> <inline classes="operator">+</inline> <inline classes="name other">layerCollection</inline><inline classes="punctuation">.</inline><inline classes="name other">length</inline><inline classes="punctuation">);</inline>
<inline classes="keyword declaration">var</inline> <inline classes="name other">anAVItem</inline> <inline classes="operator">=</inline> <inline classes="name other">app</inline><inline classes="punctuation">.</inline><inline classes="name other">project</inline><inline classes="punctuation">.</inline><inline classes="name other">item</inline><inline classes="punctuation">(</inline><inline classes="literal number integer">2</inline><inline classes="punctuation">);</inline>
<inline classes="name other">layerCollection</inline><inline classes="punctuation">.</inline><inline classes="name other">add</inline><inline classes="punctuation">(</inline><inline classes="name other">anAVItem</inline><inline classes="punctuation">);</inline>
<inline classes="name other">alert</inline><inline classes="punctuation">(</inline><inline classes="literal string double">"number of layers after is "</inline> <inline classes="operator">+</inline> <inline classes="name other">layerCollection</inline><inline classes="punctuation">.</inline><inline classes="name other">length</inline><inline classes="punctuation">);</inline></literal_block>
