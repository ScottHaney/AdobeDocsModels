<inline classes="keyword declaration">var</inline> <inline classes="name other">axesData</inline> <inline classes="operator">=</inline> <inline classes="name other">firstVariableFont</inline><inline classes="punctuation">.</inline><inline classes="name other">designAxesData</inline><inline classes="punctuation">;</inline>

<inline classes="comment single">// Getting the first design axis for that Font
</inline><inline classes="keyword declaration">var</inline> <inline classes="name other">firstAxis</inline> <inline classes="operator">=</inline> <inline classes="name other">axesData</inline><inline classes="punctuation">[</inline><inline classes="literal number integer">0</inline><inline classes="punctuation">];</inline>

<inline classes="name other">alert</inline><inline classes="punctuation">(</inline><inline classes="name other">firstAxis</inline><inline classes="punctuation">.</inline><inline classes="name other">name</inline><inline classes="operator">+</inline><inline classes="literal string double">"\n"</inline><inline classes="operator">+</inline><inline classes="name other">firstAxis</inline><inline classes="punctuation">.</inline><inline classes="name other">tag</inline><inline classes="operator">+</inline><inline classes="literal string double">"\n"</inline><inline classes="operator">+</inline><inline classes="name other">firstAxis</inline><inline classes="punctuation">.</inline><inline classes="name other">min</inline><inline classes="operator">+</inline><inline classes="literal string double">"\n"</inline><inline classes="operator">+</inline><inline classes="name other">firstAxis</inline><inline classes="punctuation">.</inline><inline classes="name other">max</inline><inline classes="punctuation">);</inline></literal_block>
